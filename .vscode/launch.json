{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Robotik-tests with gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Robotik-tests",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                }
            ]
        },
        {
            "name": "Robotik-viewer with gdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Robotik-Viewer",
            "args": [
                "${workspaceFolder}/data/scara_robot.urdf"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                }
            ]
        }
    ]
}